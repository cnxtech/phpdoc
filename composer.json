{
    "name": "codex/phpdoc",
    "type": "codex-addon",
    "description": "The Phpdoc addon provides Codex the means to parse the phpdoc generated xml file and integrate documenation in a user-friendly way.",
    "keywords": ["laravel", "documentation", "docs", "markdown", "codex", "phpdoc"],
    "homepage": "http://codex-project.ninja",
    "authors": [{"name": "Robin Radic", "email": "robin@radic.nl"}],
    "license": "MIT",
    "support": {
        "docs": "http://codex-project.ninja/documentation/codex-addon-phpdoc",
        "email": "robin@radic.nl",
        "issues": "https://github.com/codex-project/addon-phpdoc/issues",
        "source": "https://github.com/codex-project/addon-phpdoc"
    },
    "require": {
        "codex/core": "~2.1",
        "jms/serializer": "~1.9",
        "phpdocumentor/reflection-docblock": "~3.0|~4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "friendsofphp/php-cs-fixer": "~2.7",
        "mockery/mockery": "~0.9",
        "orchestra/testbench": "3.5.*",
        "graham-campbell/testbench": "~4.0",
        "superbalist/flysystem-google-storage":"~5.0"
    },
    "autoload": {
        "psr-4": {
            "Codex\\Phpdoc\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Codex\\Tests\\Phpdoc\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover":[
                "codex/phpdoc"
            ]
        },
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
